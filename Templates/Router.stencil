import Foundation
import Swiftkiq

class Router: Routable {
    func dispatch(_ work: UnitOfWork) throws {
        switch work.workerClass {
        {% for type in types.based.WorkerType %}
        case "{{ type.name }}":
            try invokeWorker(workerClass: {{ type.name }}.self, work: work)
        {% endfor %}
        default:
            break
        }
    }
}
